{
    "name": "@1szx1/vite-plugin-swagger2ts",
    "description": "fork for https://github.com/1SZX1/vite-plugin-swagger2ts",
    "version": "1.0.19",
    "license": "MIT",
    "author": "1SZX1 <2414126198@qq.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/1SZX1/vite-plugin-swagger2ts"
    },
    "homepage": "https://github.com/1SZX1/vite-plugin-swagger2ts",
    "bugs": "https://github.com/1SZX1/vite-plugin-swagger2ts/issues",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        },
        "./adapters": {
            "require": "./dist/adapters.js",
            "import": "./dist/adapters.mjs",
            "types": "./dist/adapters.d.ts"
        }
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*"
            ]
        }
    },
    "scripts": {
        "dev": "pnpm run build -- --watch --ignore-watch examples",
        "build": "tsup src/index.ts src/adapters.ts --dts --format cjs,esm",
        "prepublishOnly": "pnpm run build",
        "release": "pnpx git-ensure -a && pnpx bumpp --commit --tag --push",
        "demo:dev": "pnpm -C examples/demo run dev",
        "demo:build": "pnpm -C examples/demo run build",
        "demo:preview": "pnpm -C examples/demo run preview",
        "test": "jest",
        "lint": "eslint --fix --ext .ts ./",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
        "preinstall": "npx only-allow pnpm"
    },
    "dependencies": {
        "cross-fetch": "^3.1.5",
        "openapi3-ts": "^2.0.2",
        "swagger2openapi": "^7.0.8"
    },
    "peerDependencies": {
        "vite": "^2.0.0"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^0.6.6",
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@dcloudio/types": "^3.2.11",
        "@types/jest": "^26.0.24",
        "@types/node": "^14.18.12",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^5.14.0",
        "axios": "^1.3.4",
        "conventional-changelog": "^3.1.25",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.32.0",
        "husky": "^8.0.3",
        "jest": "^26.6.3",
        "jest-esbuild": "^0.1.6",
        "lint-staged": "^10.5.4",
        "luch-request": "^3.0.8",
        "rollup": "^2.70.0",
        "ts-node": "^9.1.1",
        "tsup": "^6.6.3",
        "typescript": "^4.6.2",
        "vite": "^2.8.6"
    },
    "lint-staged": {
        "*.{ts}": "eslint --fix --ext",
        "*.{ts,md}": "prettier --write"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}