{
    "name": "@1szx1/vite-plugin-swagger2ts",
    "description": "forked from https://github.com/morelearn1990/vite-plugin-swagger2ts",
    "version": "1.3.7",
    "license": "MIT",
    "author": "1SZX1 <2414126198@qq.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/1SZX1/vite-plugin-swagger2ts"
    },
    "homepage": "https://github.com/1SZX1/vite-plugin-swagger2ts",
    "bugs": "https://github.com/1SZX1/vite-plugin-swagger2ts/issues",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        },
        "./adapters": {
            "require": "./dist/adapters.cjs",
            "import": "./dist/adapters.mjs",
            "types": "./dist/adapters.d.ts"
        }
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "typesVersions": {
        "*": {
            "adapters": [
                "./dist/adapters"
            ]
        }
    },
    "bin": {
        "swagger2ts": "./dist/bin.mjs"
    },
    "scripts": {
        "build": "unbuild",
        "stub": "unbuild --stub",
        "prepublishOnly": "pnpm run build",
        "release": "pnpx git-ensure -a && pnpx bumpp --commit --tag --push",
        "demo:dev": "pnpm -C examples/demo run dev",
        "demo:build": "pnpm -C examples/demo run build",
        "demo:preview": "pnpm -C examples/demo run preview",
        "test": "jest",
        "lint": "eslint --fix --ext .ts ./",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
        "preinstall": "npx only-allow pnpm"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "chalk": "^4.1.2",
        "cross-fetch": "^3.1.5",
        "luch-request": "^3.1.0",
        "openapi3-ts": "^2.0.2",
        "swagger2openapi": "^7.0.8",
        "unconfig": "^0.3.9"
    },
    "peerDependencies": {
        "axios": "^1.4.0",
        "luch-request": "^3.1.0",
        "vite": "^2.0.0"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^0.6.6",
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@dcloudio/types": "^3.3.3",
        "@types/eslint": "^8.44.0",
        "@types/jest": "^26.0.24",
        "@types/node": "^14.18.12",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^5.14.0",
        "conventional-changelog": "^3.1.25",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.44.0",
        "husky": "^8.0.3",
        "jest": "^26.6.3",
        "jest-esbuild": "^0.1.6",
        "lint-staged": "^10.5.4",
        "typescript": "^4.7.0",
        "unbuild": "^2.0.0",
        "vite": "^2.8.6"
    },
    "lint-staged": {
        "*.{ts}": "eslint --fix --ext",
        "*.{ts,md}": "prettier --write"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}