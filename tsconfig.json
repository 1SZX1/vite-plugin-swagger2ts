{
    "compilerOptions": {
        "module": "ESNext",
        "target": "ESNext",
        "lib": [
            "ESNext"
        ],
        "esModuleInterop": true,
        "strict": true,
        "strictNullChecks": true,
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "noUnusedParameters": true,
        "types": [
            "@dcloudio/types"
        ]
    },
    "exclude": [
        "**/dist",
        "**/node_modules",
        "**/test"
    ]
}